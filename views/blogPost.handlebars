
<div class="text-left blogPost-list">
  <h2 ><a href="/profile">{{name}}</a></h2>
  <p>{{{get_emoji}}}</p>
  <p>{{description}}</p>
  
  <div class="blogPost-content">
    <h4 class="blogPost-name">{{blogPost.name}}</h4>
    <p class="blogPost-desc">{{blogPost.description}}</p>

  </div>
  <p>Created by {{user.name}} on {{format_date date_created}}</p>
</div>  

<script src="../public/js/blogEdit.js"></script>